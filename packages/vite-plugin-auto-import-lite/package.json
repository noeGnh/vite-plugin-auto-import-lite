{
  "name": "vite-plugin-auto-import-lite",
  "description": "Lite vite plugin for auto importing modules",
  "version": "0.0.2",
  "private": false,
  "author": "No√© Gnanih <noegnanih@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --clean",
    "lint": "eslint 'src/**/*.{ts}'",
    "version": "auto-changelog --template keepachangelog --hide-credit -p && git add CHANGELOG.md"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.2.0",
    "@vue/compiler-sfc": "^3.5.18",
    "unimport": "^5.2.0",
    "vite": "^6.3.4"
  },
  "devDependencies": {
    "auto-changelog": "^2.5.0",
    "tsup": "^8.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noeGnh/vite-plugin-auto-import-lite.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/noeGnh/vite-plugin-auto-import-lite/issues"
  },
  "homepage": "https://github.com/noeGnh/vite-plugin-auto-import-lite#readme",
  "publishConfig": {
    "access": "public"
  }
}
